[
  {
    "name": "asterisks",
    "pattern": "\\*([^\\*]+)\\*",
    "replacement": "\\1"
  },
  {
    "name": "quotes",
    "pattern": "\"([^\"]+)\"",
    "replacement": "\\1"
  },
  {
    "name": "emphasis",
    "pattern": "_([^_]+)_",
    "replacement": "\\1"
  },
  {
   "name": "Siglas",
   "pattern": "([A-Z])(?=[A-Z\\]])",
   "replacement": "\\1."
  },
  {
    "name": "Barra",
    "pattern": "\\\\(?=\\s|$)",
    "replacement": ""
  },
  {
    "name": "Chapter",
    "pattern": "^#\\s",
    "replacement": ". Abertura de capítulo. "
  },
  {
    "name": "Section", 
    "pattern": "^##\\s",
    "replacement": ". Abertura de seção. "
  },
  {
     "name": "asterisk_space",
     "pattern": "\\\\\\*\\s",
     "replacement": ""
  }
]
